{
    "manifest_version": 3,
    "name": "LinkedIn Job Description Copier",
    "version": "1.0",
    "permissions": [
      "clipboardWrite"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["*://www.linkedin.com/jobs/*"],
        "js": ["content.js"]
      }
    ]
}
